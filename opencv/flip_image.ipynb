    "import cv2 as cv"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "img = cv.imread('resources/dog.jpg')\n",
    "cv.imshow('original',img)\n",
    "img0 = cv.flip(img,0)\n",
    "img1 = cv.flip(img,1)\n",
    "img2 = cv.flip(img,-1)\n",
    "cv.imshow('flipped0',img0)\n",
    "cv.imshow('flipped1',img1)\n",
    "cv.imshow('flipped-0',img2)\n",
    "cv.waitKey(0)\n",
    "cv.destroyAllWindows()"
   ]
